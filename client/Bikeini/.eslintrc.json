{
  "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [ 1, { "extensions": [ ".js", ".jsx" ] } ],
     "react/prop-types": ["error", { "ignore": ["navigation"] }],
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-console": 0,
    "max-len": 0
  }
}